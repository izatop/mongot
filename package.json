{
  "name": "mongot",
  "version": "0.2.13",
  "description": "A lightweight typed MongoDb library for TypeScript.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "typings": "build/index.d.ts",
  "scripts": {
    "build": "rimraf build && tsc",
