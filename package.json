  "license": "MIT",
  "bugs": {
    "url": "https://github.com/izatop/mongot/issues"
  },
  "homepage": "https://github.com/izatop/mongot#readme",
  "dependencies": {
    "@types/mongodb": "^2.1.34",
    "mongodb": "^2.2.11"
  },
  "devDependencies": {
    "@types/tape": "^4.2.27",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.2",
    "tape": "^4.6.0",
    "typescript": "^2.0.3"
  }
